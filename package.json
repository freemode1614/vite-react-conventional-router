{
  "name": "@moccona/vite-react-conventional-router",
  "version": "0.0.0",
  "description": "Vite plugin for react conventional router",
  "type": "module",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsup --watch",
    "build": "cross-env NODE_ENV=production tsup",
    "e2e": "playwright test",
    "example:react": "pnpm --filter react-example run dev"
  },
  "keywords": [],
  "author": "wp.l",
  "license": "MIT",
  "exports": {
    "./package.json": "./package.json",
    "./client": {
      "types": "./npm/client.d.ts"
    },
    ".": {
      "types": "./npm/index.d.ts",
      "import": "./npm/index.js",
      "require": "./npm/index.cjs"
    }
  },
  "dependencies": {
    "debug": "^4.3.7",
    "fast-glob": "^3.3.2",
    "glob-to-regexp": "^0.4.1"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.8",
    "@moccona/eslint-config": "^0.3.5",
    "@moccona/prettierrc": "^0.0.3",
    "@playwright/test": "^1.47.1",
    "@types/debug": "^4.1.12",
    "@types/glob-to-regexp": "^0.4.4",
    "@types/node": "^20.16.5",
    "@types/react": "^18.3.3",
    "cross-env": "^7.0.3",
    "playwright": "^1.47.1",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-router": "^6.26.2",
    "tsup": "^8.3.0",
    "typescript": "^5.6.2",
    "vite": "^5.4.6",
    "vitest": "^2.1.1"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-router": "^6.26.2",
    "vite": "5"
  },
  "prettier": "@moccona/prettierrc",
  "packageManager": "pnpm@9.7.1"
}
